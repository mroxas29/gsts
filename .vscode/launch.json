  "version": "0.2.0",
  "configurations": [
    {
      "name": "gsts",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "gsts (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--web-port=5000"]
    },
    {
      "name": "gsts (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--web-port=5000"]

    },
    {
      "name": "Flutter (lib\\main.dart)",
      "type": "dart",
      "request": "launch",
      "program": "lib\\main.dart"
    }
  ]
}
